{
  "name": "SurveyProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "setDevelopment": "set ENVFILE=.env.dev",
    "setStaging": "set ENVFILE=.env.stage",
    "setProduction": "set ENVFILE=.env.prod",
    "android:dev": "npm run setDevelopment && react-native run-android --mode=devDebug --appIdSuffix=dev",
    "android:stage": "npm run setStaging && react-native run-android --mode=stageDebug --appIdSuffix=stage",
    "android:prod": "npm run setProduction && react-native run-android --mode=prodDebug --appIdSuffix=prod",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/client": "^3.9.8",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "graphql": "^16.8.1",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-config": "^1.5.1",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-vector-icons": "^10.0.3",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
